    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <ImageView
        android:id="@+id/thermometer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/background_thermometer"
        app:layout_constraintDimensionRatio="w,29:289"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.11" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/end_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.91" />

    <TextView
        android:id="@+id/temperature_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" 5 "
        android:textColor="@color/black"
        android:textSize="@dimen/thermometer_temperature_number_size"
        app:layout_constraintBottom_toBottomOf="@+id/thermometer"
        app:layout_constraintEnd_toStartOf="@id/temperature_10"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="@+id/start_guideline"
        app:layout_constraintTop_toTopOf="@+id/thermometer"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/temperature_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textColor="@color/black"
        android:textSize="@dimen/thermometer_temperature_number_size"
        app:layout_constraintBottom_toBottomOf="@+id/thermometer"
        app:layout_constraintEnd_toStartOf="@+id/temperature_15"
        app:layout_constraintStart_toEndOf="@+id/temperature_5"
        app:layout_constraintTop_toTopOf="@+id/thermometer"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/temperature_15"
