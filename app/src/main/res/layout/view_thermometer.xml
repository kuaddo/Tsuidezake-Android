        android:textSize="@dimen/thermometer_temperature_number_size"
        app:layout_constraintBottom_toBottomOf="@+id/thermometer"
        app:layout_constraintEnd_toStartOf="@+id/temperature_20"
        app:layout_constraintStart_toEndOf="@+id/temperature_10"
        app:layout_constraintTop_toTopOf="@+id/thermometer"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/temperature_20"
        android:layout_width="wrap_content"
